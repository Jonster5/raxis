{
	"name": "raxis-ecs",
	"version": "1.0.4",
	"description": "Raxis Game Engine",
	"type": "module",
	"files": [
		"dist"
	],
	"main": "./dist/raxis-main.cjs.js",
	"module": "./dist/raxis-main.es.js",
	"types": "./dist/main.d.ts",
	"exports": {
		".": {
			"import": "./dist/raxis-main.es.js",
			"require": "./dist/raxis-main.cjs.js",
			"types": "./dist/main.d.ts"
		},
		"./engine": {
			"import": "./dist/raxis-engine.es.js",
			"require": "./dist/raxis-engine.cjs.js",
			"types": "./dist/engine.d.ts"
		},
		"./math": {
			"import": "./dist/raxis-math.es.js",
			"require": "./dist/raxis-math.cjs.js",
			"types": "./dist/math.d.ts"
		},
		"./plugins": {
			"import": "./dist/raxis-plugins.es.js",
			"require": "./dist/raxis-plugins.cjs.js",
			"types": "./dist/plugins.d.ts"
		}
	},
	"scripts": {
		"dev": "vite",
		"docs": "documentation build src/** -f md -o docs.md",
		"build": "npm run docs & tsc && vite build",
		"publish": "npm run build & npm publish",
		"version": "git add -A . && git commit",
		"postversion": "git push && git push --tags"
	},
	"devDependencies": {
		"execa": "^7.1.1",
		"typescript": "^5.1.3",
		"vite": "^4.3.9",
		"vite-plugin-dts": "^3.0.2"
	},
	"repository": "https://github.com/Jonster5/raxis.git",
	"dependencies": {
		"number-allocator": "^1.0.14"
	}
}
