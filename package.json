{
	"name": "raxis",
	"version": "1.3.2",
	"description": "Raxis Game Engine",
	"type": "module",
	"files": [
		"dist"
	],
	"main": "./dist/raxis-main.cjs",
	"module": "./dist/raxis-main.js",
	"types": "./dist/main.d.ts",
	"exports": {
		".": {
			"import": "./dist/raxis-main.js",
			"require": "./dist/raxis-main.cjs",
			"types": "./dist/main.d.ts"
		},
		"./math": {
			"import": "./dist/raxis-math.js",
			"require": "./dist/raxis-math.cjs",
			"types": "./dist/math.d.ts"
		}
	},
	"scripts": {
		"dev": "vite",
		"docs": "documentation build src/** -f md -o ./docs/docs.md && documentation build src/** -f html -o site",
		"build": "npm run docs & tsc && vite build",
		"publish": "npm run build && npm publish"
	},
	"devDependencies": {
		"@types/node": "^20.3.3",
		"execa": "^7.1.1",
		"number-allocator": "^1.0.14",
		"typescript": "^5.1.3",
		"vite": "^4.3.9",
		"vite-plugin-dts": "^3.0.2"
	},
	"repository": "https://github.com/Jonster5/raxis.git"
}
